<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="ForegroundColor">White</Color>
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}" />
    
    <Color x:Key="HighlightColor">#b8b8b8</Color>
    <SolidColorBrush x:Key="HighlightBrush" Color="{StaticResource HighlightColor}" />
    
    <Color x:Key="BackgroundColor">#1a1a1a</Color>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}" />

    <Color x:Key="MediumColor">#262626</Color>
    <SolidColorBrush x:Key="MediumBrush" Color="{StaticResource MediumColor}" />

    <Color x:Key="DimColor">#707070</Color>
    <SolidColorBrush x:Key="DimBrush" Color="{StaticResource DimColor}" />
</ResourceDictionary>